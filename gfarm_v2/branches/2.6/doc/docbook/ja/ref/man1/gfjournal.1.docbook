<refentry id='gfjournaladmin.1'>

<refentryinfo><date>2 Jul 2012</date></refentryinfo>

<refmeta>
<refentrytitle>gfjournal</refentrytitle>
<manvolnum>1</manvolnum>
<refmiscinfo>Gfarm</refmiscinfo>
