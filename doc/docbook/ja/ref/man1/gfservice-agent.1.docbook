<varlistentry>
<term>
<command>gfcmd</command>
<parameter>cmd</parameter>=<replaceable>コマンド名</replaceable>
<parameter>args</parameter>="<replaceable>コマンドの引数 ...</replaceable>"
</term>
<listitem>
